<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.company %></td>
        <td><%= listing.title %></td>
        <td><%= listing.erp %></td>
        <td><%= listing.publish_on %></td>
        <td><%= link_to 'Show', listing_path(listing.slug) %></td>
        <td><%= link_to 'Edit', edit_job_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', job_listing_path(listing), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_job_listing_path %>
